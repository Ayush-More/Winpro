                });

                // Generate random member counts and winning amounts
                function generateRandomMemberCount() {
                    // Generate random number between 100,000 and 999,999
                    return Math.floor(Math.random() * (999999 - 100000 + 1)) + 100000;
                }

                function generateRandomWinningAmount() {
                    // Generate random number between 100,000 and 999,999
                    return Math.floor(Math.random() * (999999 - 100000 + 1)) + 100000;
                }

                function updateRandomValues() {
                    // Update all member counts
                    const memberElements = [
                        'member-wingo',
                        'member-k3',
                        'member-5d',
                        'member-trx',
                        'member-aviator'
                    ];

                    // Update all winning amounts
                    const winningElements = [
                        'winning-wingo',
                        'winning-k3',
                        'winning-5d',
                        'winning-trx',
                        'winning-aviator'
                    ];

                    // Set random values for member counts
                    memberElements.forEach(elementId => {
                        const element = document.getElementById(elementId);
                        if (element) {
                            element.textContent = generateRandomMemberCount().toLocaleString();
                        }
                    });

                    // Set random values for winning amounts
                    winningElements.forEach(elementId => {
                        const element = document.getElementById(elementId);
                        if (element) {
                            element.textContent = generateRandomWinningAmount().toLocaleString();
                        }
                    });
                }

                // Initialize random values on page load
                updateRandomValues();

                // Update random values every 30 seconds to make it more dynamic
                setInterval(updateRandomValues, 30000);

                // Show main popup after 2 seconds (only if popup exists)
                const mainPopup = document.getElementById('mainPopup');
                if (mainPopup) {
                    setTimeout(function() {
                        mainPopup.classList.add('active');
                    }, 2000);
                }

                // Show notification after 4 seconds (only if notification exists)
                const notification = document.getElementById('notification');
                if (notification) {
                    setTimeout(function() {
                        notification.classList.add('show');

                        // Hide notification after 5 seconds
                        setTimeout(function() {
                            notification.classList.remove('show');
                        }, 5000);
                    }, 4000);
                }

                // Close popup functionality (only if elements exist)
                const closePopup = document.getElementById('closePopup');
                if (closePopup && mainPopup) {
                    closePopup.addEventListener('click', function() {
                        mainPopup.classList.remove('active');
                    });
                }

                // Claim offer button (only if elements exist)
                const claimOffer = document.getElementById('claimOffer');
                if (claimOffer && mainPopup) {
                    claimOffer.addEventListener('click', function() {
                        alert('Bonus claimed successfully!');
                        mainPopup.classList.remove('active');
                    });
                }



                // Game card interactions
                const gameCards = document.querySelectorAll('.jili_games_card');
                gameCards.forEach(card => {
                    card.addEventListener('click', function(e) {
                        e.preventDefault();
                        alert('Game loading...');
                    });
                });

                // Category card interactions
                const categoryCards = document.querySelectorAll('.card');
                categoryCards.forEach(card => {
                    card.addEventListener('click', function(e) {
                        if (this.getAttribute('href') === '#') {
                            e.preventDefault();
                            alert('Category selected!');
                        }
                    });
                });

                // Footer navigation
                const navItems = document.querySelectorAll('.tabbar__container-item');
                navItems.forEach(item => {
                    item.addEventListener('click', function() {
                        navItems.forEach(i => i.classList.remove('active'));
                        this.classList.add('active');
                    });
                });

                // Bottom bar animation and detail button
                const bottomBar = document.querySelector('.bottombar');
                const detailButton = document.querySelector('.bottombar .mainbtn');

                if (detailButton) {
                    detailButton.addEventListener('click', function(e) {
                        e.preventDefault();
                        e.stopPropagation();

                        // Show detailed announcement information
                        const currentText = middleContent ? middleContent.textContent : announcements[0];
                        const detailInfo = `
                            📢 Announcement Details:

                            ${currentText}

                            🕒 Posted: ${new Date().toLocaleDateString()}
                            📍 Category: General Announcement
                            🎯 Target: All Users

                            Thank you for being part of Dubaideck!
                        `;

                        alert(detailInfo);
                    });
                }

                // Auto-scrolling for middle content
                const middleContent = document.querySelector('.middlecontent p');

                // Array of dummy announcements
                const announcements = [
                    "🎉 Welcome to Dubaideck! Enjoy exciting games and win big rewards! 💰",
                    "🎁 New users get special bonuses! Register now and claim your rewards!",
                    "🏆 Daily tournaments with amazing prizes! Join now and compete!",
                    "💎 VIP members get exclusive benefits and higher rewards!",
                    "🎮 Play responsibly and have fun! Good luck to all players!",
                    "🔥 Hot games available now! Try your luck and win big!",
                    "⭐ Rate us 5 stars and get bonus coins! Thank you for your support!",
                    "🚀 New games added weekly! Check out our latest additions!",
                    "💸 Instant withdrawals available 24/7! Fast and secure!",
                    "🎊 Weekend special offers! Double your winnings this weekend!",
                    "🔒 Your security is our priority! 100% safe and encrypted!",
                    "📱 Download our mobile app for better gaming experience!"
                ];

                let currentAnnouncementIndex = 0;

                // Function to update announcement text with fade effect
                function updateAnnouncement() {
                    if (middleContent) {
                        // Fade out
                        middleContent.style.opacity = '0';

                        setTimeout(() => {
                            // Change text
                            middleContent.textContent = announcements[currentAnnouncementIndex];
                            currentAnnouncementIndex = (currentAnnouncementIndex + 1) % announcements.length;

                            // Fade in
                            middleContent.style.opacity = '1';
                        }, 250);
                    }
                }

                // Start with first announcement
                if (middleContent) {
                    middleContent.textContent = announcements[0];
                    currentAnnouncementIndex = 1;
                }

                // Change announcement every 8 seconds (to allow full scroll animation)
                setInterval(updateAnnouncement, 8000);

                // Check user login status and handle UI accordingly
                checkUserLoginAndUpdateUI();
            });

            // Function to check user login status and update UI accordingly
            function checkUserLoginAndUpdateUI() {
                console.log('checkUserLoginAndUpdateUI function called');

              

                // If auth cookie exists, try to get user info
                fetch('/api/webapi/GetUserInfo', {
                        method: 'GET',
                        credentials: 'include', // Include cookies
                        headers: {
                            'Content-Type': 'application/json',
                        }
                    })
                    .then(response => {
                        console.log('API response status:', response.status);
                        if (response.status === 302 || response.redirected) {
                            console.log('User redirected to login, not authenticated');
                            showLoginButton();
                            return null;
                        }
                        return response.json();
                    })
                    .then(data => {
                        if (data && data.status === true && data.data) {
                            // User is logged in
                            const userName = data.data.name_user || 'Member';
                            console.log('User is logged in:', userName);

                            // Hide login button and show user info
                            hideLoginButtonShowUserInfo(userName);

                            // Show welcome blast animation (only once per session)
                            if (!sessionStorage.getItem('welcomeBlastShown')) {
                                showWelcomeBlast(userName);
                                sessionStorage.setItem('welcomeBlastShown', 'true');
                            }
                        } else {
                            // User is not logged in, show login button
                            console.log('User is not logged in or invalid response');
                            showLoginButton();
                        }
                    })
                    .catch(error => {
                        console.log('Error checking login status:', error);
                        // Show login button on error (user not logged in)
                        showLoginButton();
                    });
            }

            // Helper function to get cookie value
            function getCookie(name) {
                const value = `; ${document.cookie}`;
                const parts = value.split(`; ${name}=`);
                if (parts.length === 2) return parts.pop().split(';').shift();
                return null;
            }

            // Function to hide login button and show user info
            function hideLoginButtonShowUserInfo(userName) {
                console.log('hideLoginButtonShowUserInfo called for:', userName);

                const loginButton = document.getElementById('loginButton');
                const downloadButton = document.getElementById('downloadButton');
                const userInfo = document.getElementById('userInfo');
                const userWelcome = document.getElementById('userWelcome');

                console.log('Elements found:', {
                    loginButton: !!loginButton,
                    downloadButton: !!downloadButton,
                    userInfo: !!userInfo,
                    userWelcome: !!userWelcome
                });

                if (loginButton) {
                    loginButton.style.display = 'none';
                    console.log('Login button hidden');
                }
                  if (downloadButton) {
                    downloadButton.style.display = 'none';
                }

                if (userInfo) {
                    userInfo.style.display = 'flex';
                }


                if (userWelcome) {
                    userWelcome.textContent = `Welcome, ${userName}!`;
                }




            }

            // Function to show login button (for non-logged in users)
            function showLoginButton() {
                console.log('showLoginButton called');

                const loginButton = document.getElementById('loginButton');
                const downloadButton = document.getElementById('downloadButton');
                const userInfo = document.getElementById('userInfo');

                console.log('Elements found:', {
                    loginButton: !!loginButton,
                    downloadButton: !!downloadButton,
                    userInfo: !!userInfo
                });

                if (loginButton) {
                    loginButton.style.display = 'inline-block';
                    console.log('Login button shown');
                }

                if (downloadButton) {
                    downloadButton.style.display = 'inline-block';
                    console.log('Download button shown');
                }

                if (userInfo) {
                    userInfo.style.display = 'none';
                    console.log('User info hidden');
                }
            }

            // Function to show the welcome blast animation
            function showWelcomeBlast(userName) {
                const blastContainer = document.getElementById('welcomeBlastContainer');
                const welcomeText = document.getElementById('welcomeText');
                const particlesContainer = document.getElementById('blastParticles');



                // Show the blast container
                blastContainer.style.display = 'block';

                // Create particles for the blast effect
                createBlastParticles(particlesContainer);

                // Create sparkles
                createSparkles(blastContainer);

                // Hide the animation after 4 seconds
                setTimeout(() => {
                    blastContainer.classList.add('fade-out-animation');
                    setTimeout(() => {
                        blastContainer.style.display = 'none';
                        blastContainer.classList.remove('fade-out-animation');
                    }, 1000);
                }, 4000);
            }

            // Function to create blast particles
            function createBlastParticles(container) {
                const particleCount = 50;

                for (let i = 0; i < particleCount; i++) {
                    const particle = document.createElement('div');
                    particle.className = 'particle';

                    // Random position around the center
                    const angle = (Math.PI * 2 * i) / particleCount;
                    const distance = Math.random() * 200 + 50;
                    const x = Math.cos(angle) * distance;
                    const y = Math.sin(angle) * distance;

                    particle.style.left = x + 'px';
                    particle.style.top = y + 'px';

                    // Random colors
                    const colors = ['#FFD700', '#FF6B35', '#00BFFF', '#FF1493', '#32CD32', '#FF4500'];
                    particle.style.background = colors[Math.floor(Math.random() * colors.length)];

                    // Random animation delay
                    particle.style.animationDelay = Math.random() * 0.5 + 's';

                    container.appendChild(particle);
                }

                // Clean up particles after animation
                setTimeout(() => {
                    container.innerHTML = '';
                }, 3000);
            }

            // Function to create sparkle effects
            function createSparkles(container) {
                const sparkleCount = 30;

                for (let i = 0; i < sparkleCount; i++) {
                    const sparkle = document.createElement('div');
                    sparkle.className = 'sparkle';

                    // Random position
                    sparkle.style.left = Math.random() * 100 + '%';
                    sparkle.style.top = Math.random() * 100 + '%';

                    // Random animation delay
                    sparkle.style.animationDelay = Math.random() * 2 + 's';

                    container.appendChild(sparkle);
                }

                // Clean up sparkles after animation
                setTimeout(() => {
                    const sparkles = container.querySelectorAll('.sparkle');
                    sparkles.forEach(sparkle => sparkle.remove());
                }, 4000);
            }
            
            
