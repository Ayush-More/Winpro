            if (!username || !password) {
                alertMess(text);
                return false;
            } else {
                return true;
            }
        }

        $('button').click(async(e) => {
            e.preventDefault();
            let username = $('input:eq(0)').val().trim();
            let password = $('input:eq(1)').val().trim();

            {
                $('.van-overlay').fadeIn(10);
                $.ajax({
                    type: "POST",
                    url: "/api/webapi/login",
                    data: {
                        username: username,
                        pwd: password,
                    },
                    dataType: "json",
                    success: function(response) {


                        //console.log(response);
                        if (response.status === true) {
                            $('.van-overlay').fadeOut(300);
                            $('.Loading').fadeIn(10);
                            setCookie('token', response.token, 1);
                            setCookie('auth', response.value, 1);

                            // alertMess(response.message);
                            setTimeout(() => {
                                location.href = '/home';
                            }, 500);
                        } else {
                            $('.van-overlay').fadeOut(300);
                            alertMess(response.message);

                        }
                    }
                });
            }

        });
    </script>
